 <h1> All Consoles </h1>
 <%= link_to "New Console", new_console_path %>

 <% @consoles.each do |console| %>
 <%= console.developer %>
 <hr>
 <%= console.release_year %>
 <hr>
 <%= console.description %>
 <%= link_to "Edit Console", edit_console_path(console) %>
 <%= link_to "Show Console", console_path(console) %>
 <%= link_to "Delete", console_path(console), method: :delete, data: {confirm: "Are you sure you want to delete"}%>

 <% end %>
